<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WeChatCrop">
        <attr name="clipType" format="enum">
            <enum name="rect" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ShapeImageView">
        <attr name="shapeType" format="enum">
            <enum name="love" value="2" />
            <enum name="round" value="1" />
            <enum name="circle" value="0" />
            <enum name="fiveStart" value="3" />
        </attr>

        <attr name="scaleType" format="enum">
            <enum name="fitXY" value="0" />
            <enum name="center" value="3" />
            <enum name="fitCenter" value="1" />
            <enum name="centerCrop" value="2" />
            <enum name="centerInside" value="4" />
        </attr>

        <attr name="src" format="reference" />
        <attr name="stroke" format="boolean" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="round_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="loadingColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <attr name="backgroundAlpha" format="integer" />
    </declare-styleable>

</resources>