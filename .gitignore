#此方式可以实现忽略一个文件夹中的部分文件夹
#首先忽略一个目录下的所有,然后再指定不忽略的文件夹,然后再忽略这个文件夹中的所有,以此类推
#不忽略可以指定多个.

/src/main/*
!/src/main/res/
!/src/main/java/
!/src/main/assets/

/src/main/assets/*
#Insert here is not ignored assets files.
!/src/main/assets/grade.json
!/src/main/assets/material_loading2.json



/src/main/java/*
#Insert here is not ignored java files.
!/src/main/java/my



/src/main/res/*
!/src/main/res/menu/
!/src/main/res/layout/
!/src/main/res/values/
!/src/main/res/drawable/


/src/main/res/layout/*
#Insert here is not ignored layout files.
!/src/main/res/layout/activity_photo.xml
!/src/main/res/layout/collapsing_toolbar_layout.xml
!/src/main/res/layout/net_work_not_connect_hint.xml
!/src/main/res/layout/batch_delete_bottom_layout.xml




/src/main/res/drawable/*
#Insert here is not ignored drawable files.
!/src/main/res/drawable/grade.png
!/src/main/res/drawable/grade2.png
!/src/main/res/drawable/sign_point.png
!/src/main/res/drawable/right_arrow.png
!/src/main/res/layout/load_more_layout.xml
!/src/main/res/drawable/select_all_button_selector.xml
!/src/main/res/drawable/net_work_listener_selected.xml
!/src/main/res/drawable/batch_delete_button_selector.xml





/src/main/res/values/*
#Insert here is not ignored values.
!/src/main/res/values/styles.xml
!/src/main/res/values/my_custom_view_attrs.xml



/src/main/res/menu/*
#Insert here is not ignored menu files.
!/src/main/res/menu/crop_menu.xml
